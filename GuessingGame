import random

print("I'm thinking of a number between", 1, " and ", 11, ".")

for i in range(50):

    number = random.randrange(1,11)

    answer = float(input("Enter your guess: "))

    if answer == number:
        print("Congrats! Today is your lucky day!")
    elif answer < number:
        print("Incorrect. Go higher!")
    else:
        print("Incorrect. Go lower!")
